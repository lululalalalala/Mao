{"ast":null,"code":"export default {\n  data() {\n    return {\n      formIsVisible: false,\n      checkbox: [],\n      reminders: [{\n        selected: false,\n        content: \"带希希看医生\"\n      }, {\n        selected: false,\n        content: \"带壹壹绝育\"\n      }],\n      reminder: {\n        selected: false,\n        content: null\n      }\n    };\n  },\n\n  methods: {\n    alterFormVisibility() {\n      this.formIsVisible = true;\n    },\n\n    alterSelected(index) {\n      this.reminders[index].selected = !this.reminders[index].selected;\n      console.log(this.reminders[index]);\n    },\n\n    addReminder() {\n      if (this.reminder.content) {\n        this.reminders.push(this.reminder);\n        this.formIsVisible = false;\n      } else {\n        if (confirm(\"提醒内容不能为空！\") === false) {\n          this.formIsVisible = false;\n        }\n      }\n    },\n\n    deleteReminder() {\n      this.reminders = this.reminders.filter(reminder => reminder.selected === false);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,KADV;MAELC,QAAQ,EAAE,EAFL;MAGLC,SAAS,EAAE,CACT;QACEC,QAAQ,EAAE,KADZ;QAEEC,OAAO,EAAE;MAFX,CADS,EAKT;QACED,QAAQ,EAAE,KADZ;QAEEC,OAAO,EAAE;MAFX,CALS,CAHN;MAaLC,QAAQ,EAAE;QACRF,QAAQ,EAAE,KADF;QAERC,OAAO,EAAE;MAFD;IAbL,CAAP;EAkBD,CApBY;;EAqBbE,OAAO,EAAE;IACPC,mBAAmB,GAAG;MACpB,KAAKP,aAAL,GAAqB,IAArB;IACD,CAHM;;IAIPQ,aAAa,CAACC,KAAD,EAAQ;MACnB,KAAKP,SAAL,CAAeO,KAAf,EAAsBN,QAAtB,GAAiC,CAAC,KAAKD,SAAL,CAAeO,KAAf,EAAsBN,QAAxD;MACAO,OAAO,CAACC,GAAR,CAAY,KAAKT,SAAL,CAAeO,KAAf,CAAZ;IACD,CAPM;;IAQPG,WAAW,GAAG;MACZ,IAAI,KAAKP,QAAL,CAAcD,OAAlB,EAA2B;QACzB,KAAKF,SAAL,CAAeW,IAAf,CAAoB,KAAKR,QAAzB;QACA,KAAKL,aAAL,GAAqB,KAArB;MACF,CAHA,MAGO;QACL,IAAIc,OAAO,CAAC,WAAD,CAAP,KAAyB,KAA7B,EAAoC;UAClC,KAAKd,aAAL,GAAqB,KAArB;QACF;MACF;IACD,CAjBM;;IAkBPe,cAAc,GAAG;MACf,KAAKb,SAAL,GAAiB,KAAKA,SAAL,CAAec,MAAf,CACdX,QAAD,IAAcA,QAAQ,CAACF,QAAT,KAAsB,KADrB,CAAjB;IAGD;;EAtBM;AArBI,CAAf","names":["data","formIsVisible","checkbox","reminders","selected","content","reminder","methods","alterFormVisibility","alterSelected","index","console","log","addReminder","push","confirm","deleteReminder","filter"],"sourceRoot":"","sources":["D:\\vue\\Mao\\src\\components\\layouts\\TheReminder.vue"],"sourcesContent":["<template>\r\n  <div class=\"the-reminder-wrapper\">\r\n    <list-sets>\r\n      <h3>提醒</h3>\r\n      <li v-for=\"(reminder, index) in reminders\" :key=\"index\">\r\n        <input\r\n          class=\"checkbox\"\r\n          type=\"checkbox\"\r\n          @click=\"alterSelected(index)\"\r\n          :checked=\"reminder.selected\"\r\n          :value=\"reminder.content\"\r\n        />{{ reminder.content }}\r\n      </li>\r\n      <form @submit.prevent v-if=\"formIsVisible\">\r\n        <input\r\n          class=\"checkbox\"\r\n          type=\"checkbox\"\r\n          @change=\"alterSelected(index)\"\r\n          :checked=\"reminder.selected\"\r\n          :value=\"reminder.content\"\r\n        />\r\n        <input v-model=\"reminder.content\" />\r\n      </form>\r\n      <div>\r\n        <button-sets v-if=\"formIsVisible\" @click=\"addReminder\"\r\n          >确认</button-sets\r\n        >\r\n\r\n        <button-sets v-else @click=\"alterFormVisibility\">添加</button-sets>\r\n\r\n        <button-sets @click=\"deleteReminder\">删除</button-sets>\r\n      </div>\r\n    </list-sets>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      formIsVisible: false,\r\n      checkbox: [],\r\n      reminders: [\r\n        {\r\n          selected: false,\r\n          content: \"带希希看医生\",\r\n        },\r\n        {\r\n          selected: false,\r\n          content: \"带壹壹绝育\",\r\n        },\r\n      ],\r\n      reminder: {\r\n        selected: false,\r\n        content: null,\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    alterFormVisibility() {\r\n      this.formIsVisible = true;\r\n    },\r\n    alterSelected(index) {\r\n      this.reminders[index].selected = !this.reminders[index].selected;\r\n      console.log(this.reminders[index]);\r\n    },\r\n    addReminder() {\r\n      if (this.reminder.content) {\r\n        this.reminders.push(this.reminder);\r\n        this.formIsVisible = false;\r\n      } else {\r\n        if (confirm(\"提醒内容不能为空！\") === false) {\r\n          this.formIsVisible = false;\r\n        }\r\n      }\r\n    },\r\n    deleteReminder() {\r\n      this.reminders = this.reminders.filter(\r\n        (reminder) => reminder.selected === false\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.the-reminder-wrapper {\r\n  width: 350px;\r\n  position: relative;\r\n  top: -1068px;\r\n  left: 845px;\r\n  text-align: center;\r\n  backdrop-filter: blur(2px);\r\n  border-radius: 30px;\r\n}\r\n.checkbox {\r\n  /* cursor: url(http://localhost:8080/3d8f8dda2aea14d3.ico), default !important; */\r\n  width: 15px;\r\n  height: 15px;\r\n  line-height: 15px;\r\n}\r\nul {\r\n  background: rgba(241, 241, 240, 0);\r\n  border: 2px solid rgba(220, 29, 29, 0.6);\r\n  box-shadow: 0 0 30px 0 rgba(220, 29, 29, 0.6);\r\n}\r\nh3 {\r\n  color: rgba(255, 1, 1, 0.682);\r\n}\r\nli {\r\n  margin-top: 20px;\r\n  color: rgb(68, 64, 64);\r\n  margin-bottom: 10px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}