\n              <button-sets @click=\"edit\">编辑</button-sets>\r\n              <button-sets @click=\"deleteAdmin\">删除</button-sets>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table-sets>\r\n    <dialog-sets\r\n      v-if=\"formIsVisible\"\r\n      title=\"请确认以下信息：\"\r\n      @close=\"addAdmin\"\r\n    >\r\n      <template #default>\r\n        <form class=\"admin-form\" v-if=\"formIsVisible\" @submit.prevent>\r\n          <label\r\n            >账号：\r\n            <input ref=\"accountInput\" />\r\n          </label>\r\n          <label\r\n            >密码：\r\n            <input ref=\"passwordInput\" />\r\n          </label>\r\n          <label\r\n            >电话：\r\n            <input ref=\"phoneInput\" />\r\n          </label>\r\n          <label\r\n            >邮箱：\r\n            <input ref=\"emailInput\" />\r\n          </label>\r\n          <label\r\n            >描述：\r\n            <textarea rows=\"3\" ref=\"descriptionInput\" />\r\n          </label>\r\n        </form>\r\n      </template>\r\n      <template #actions>\r\n        <button-sets class=\"pop-up-button\" @click=\"confirm\">确认</button-sets>\r\n        <button-sets class=\"pop-up-button\" @click=\"close\">关闭</button-sets>\r\n      </template>\r\n    </dialog-sets>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      allChecked: false,\r\n      pwdIsVisible: false,\r\n      formIsVisible: false,\r\n      accountInput: \"\",\r\n      passwordInput: \"\",\r\n      phoneInput: \"\",\r\n      emailInput: \"\",\r\n      descriptionInput: \"\",\r\n      admins: [\r\n        {\r\n          checked: false,\r\n          id: 1,\r\n          account: \"admin1\",\r\n          password: \"123456\",\r\n          role: \"管理员\",\r\n          status: true,\r\n          phone: \"18915464612\",\r\n          email: \"admin1@cats.com\",\r\n          description: \"1号管理员\",\r\n        },\r\n        {\r\n          checked: false,\r\n          id: 2,\r\n          account: \"admin2\",\r\n          password: \"123456\",\r\n          role: \"管理员\",\r\n          status: false,\r\n          phone: \"15215464612\",\r\n          email: \"admin2@cats.com\",\r\n          description: \"2号管理员\",\r\n        },\r\n        {\r\n          checked: false,\r\n          id: 3,\r\n          account: \"admin3\",\r\n          password: \"123456\",\r\n          role: \"管理员\",\r\n          status: true,\r\n          phone: \"18915468912\",\r\n          email: \"admin3@cats.com\",\r\n          description: \"3号管理员\",\r\n        },\r\n        {\r\n          checked: false,\r\n          id: 4,\r\n          account: \"admin4\",\r\n          password: \"123456\",\r\n          role: \"管理员\",\r\n          status: true,\r\n          phone: \"13615464612\",\r\n          email: \"admin4@cats.com\",\r\n          description: \"4号管理员\",\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    alterPwdVisibility() {\r\n      this.pwdIsVisible = !this.pwdIsVisible;\r\n    },\r\n    addAdmin() {\r\n      this.formIsVisible = !this.formIsVisible;\r\n    },\r\n    confirm() {\r\n      const enteredAccount = this.$refs.accountInput.value;\r\n      const enteredPassword = this.$refs.passwordInput.value;\r\n      const enteredPhone = this.$refs.phoneInput.value;\r\n      const enteredEmail = this.$refs.emailInput.value;\r\n      const enteredDescription = this.$refs.descriptionInput.value;\r\n\r\n      const newAdmin = {\r\n        checked: false,\r\n        id: this.admins.length + 1,\r\n        account: enteredAccount,\r\n        password: enteredPassword,\r\n        role: \"管理员\",\r\n        status: true,\r\n        phone: enteredPhone,\r\n        email: enteredEmail,\r\n        description: enteredDescription,\r\n      };\r\n      this.admins.push(newAdmin);\r\n      this.formIsVisible = !this.formIsVisible;\r\n    },\r\n    edit() {},\r\n    deleteAdmin() {\r\n      // const table = document.getElementsByClassName(\"table\");\r\n      const rows = document.getElementById('table-body').getElementsByTagName(\"tr\");\r\n      console.log(rows)\r\n      for (let i = 0; i < rows.length; i++) {\r\n        if (rows[i].id == this.admins[i].id) {\r\n          this.admins.splice(this.admins[i], 1);\r\n        }\r\n      }\r\n      // const adminId = document.getElementsByName(\"adminId\");\r\n      // const chosenAdmin = this.admins.find(row => adminId === row.id)\r\n      // this.admins.splice(chosenAdmin, 1)\r\n      // const adminIndex = this.admins.findIndex((admin) => admin.id === adminId);\r\n      // this.admins = this.admins.map((admin) => {\r\n      //   admin.id = adminIndex + 1;\r\n      // });\r\n    },\r\n    close() {\r\n      this.formIsVisible = !this.formIsVisible;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.the-admins-wrapper {\r\n  height: 100%;\r\n  width: 100%;\r\n  cursor: url(\"../../assets/ico/meme-pop-cat-cursor-close.ico\"), default !important;\r\n  margin-left: 230px;\r\n}\r\n.search {\r\n  padding-top: 20px;\r\n  margin-left: 30px;\r\n}\r\n.search input {\r\n  margin-right: 10px;\r\n}\r\n.search button {\r\n  margin-right: 10px;\r\n}\r\n.search button:hover {\r\n  background: radial-gradient(rgba(255, 0, 157, 0.301), transparent);\r\n  border: 1px solid rgba(255, 0, 157, 0.301);\r\n}\r\ntbody tr:nth-child(even) {\r\n  background: rgb(243, 243, 172);\r\n}\r\ntbody tr:hover {\r\n  background: rgba(255, 0, 157, 0.301);\r\n}\r\ntd {\r\n  height: 35px;\r\n  border-collapse: collapse;\r\n  border: 1px solid rgba(255, 0, 157, 0.24);\r\n}\r\nthead td {\r\n  height: 40px;\r\n  font-weight: bold;\r\n  background: radial-gradient(rgba(255, 0, 157, 0.301), transparent);\r\n  border-collapse: collapse;\r\n  border: 1px solid rgba(255, 0, 157, 0.24);\r\n  top: 0;\r\n}\r\ntd input {\r\n  width: 20px;\r\n  height: 20px;\r\n}\r\ntbody div {\r\n  width: 180px;\r\n  margin: 0 auto;\r\n  padding: 0;\r\n}\r\ntable button:first-child {\r\n  margin-right: 10px;\r\n}\r\ntd input {\r\n  cursor: url(\"../../assets/ico/meme-pop-cat-pointer-open.ico\"), pointer !important;\r\n}\r\n.password {\r\n  margin: 0 auto;\r\n  width: 80px;\r\n}\r\n.admin-form {\r\n  width: 200px;\r\n  margin: 0 auto;\r\n  margin-top: 25px;\r\n}\r\n.admin-form label {\r\n  color: rgba(255, 0, 157, 0.445);\r\n  font-weight: bold;\r\n  margin-bottom: 15px;\r\n  display: block;\r\n}\r\n.admin-form input {\r\n  text-indent: 6px;\r\n  height: 30px;\r\n  line-height: 40px;\r\n  width: 200px;\r\n  color: rgba(255, 0, 157, 0.63);\r\n  border: 1px solid rgba(255, 0, 157, 0.301);\r\n  border-radius: 8px;\r\n  font-size: 14px;\r\n  background: rgb(243, 239, 204);\r\n  margin-top: 8px;\r\n}\r\n.admin-form textarea {\r\n  text-indent: 6px;\r\n  width: 200px;\r\n  color: rgba(255, 0, 157, 0.63);\r\n  border: 1px solid rgba(255, 0, 157, 0.301);\r\n  border-radius: 8px;\r\n  font-size: 14px;\r\n  background: rgb(243, 239, 204);\r\n  margin-top: 8px;\r\n  resize: none;\r\n  font-family: sans-serif;\r\n}\r\n.admin-form input:hover,\r\ntextarea:hover {\r\n  cursor: url(\"../../assets/ico/meme-pop-cat-pointer-open.ico\"), pointer !important;\r\n}\r\n.admin-form input:focus,\r\ntextarea:focus {\r\n  outline: none;\r\n  background: rgb(243, 243, 172);\r\n}\r\n.pop-up-button {\r\n  width: 150px;\r\n  height: 50px;\r\n  margin-top: -10px;\r\n}\r\n.password div {\r\n  position: relative;\r\n  top: 20px;\r\n  left: -50px;\r\n}\r\n.password button {\r\n  margin-top: 30px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}