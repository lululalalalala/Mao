per\">\r\n    <div class=\"search\">\r\n      <input-sets type=\"search\" placeholder=\"搜索\" />\r\n      <button-sets>搜索</button-sets>\r\n      <button-sets @click=\"addAdmin\">新增</button-sets>\r\n    </div>\r\n    <table-sets id=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <td style=\"width: 30px\">\r\n            <input type=\"checkbox\" name=\"checkbox\" value=\"\" />\r\n          </td>\r\n          <td style=\"width: 30px\">序号</td>\r\n          <td style=\"width: 70px\">账号</td>\r\n          <td style=\"width: 70px\">角色</td>\r\n          <td style=\"width: 70px\">状态</td>\r\n          <td style=\"width: 100px\">电话</td>\r\n          <td style=\"width: 100px\">邮箱</td>\r\n          <td style=\"width: 150px\">描述</td>\r\n          <td style=\"width: 60px\">密码</td>\r\n          <td style=\"width: 100px\">操作</td>\r\n        </tr>\r\n      </thead>\r\n      <tbody id=\"table-body\">\r\n        <tr v-for=\"(admin, index) in admins\" :key=\"index\" :id=\"admin.id\">\r\n          <td><input type=\"checkbox\" name=\"checkbox\" value=\"\" /></td>\r\n          <td name=\"adminId\">{{ admin.id }}</td>\r\n          <td>{{ admin.account }}</td>\r\n          <td>{{ admin.role }}</td>\r\n          <td>{{ admin.status }}</td>\r\n          <td>{{ admin.phone }}</td>\r\n          <td>{{ admin.email }}</td>\r\n          <td>{{ admin.description }}</td>\r\n          <td>\r\n            <div class=\"password\">\r\n              <div>\r\n                <span v-if=\"pwdIsVisible\">{{ admin.password }}</span>\r\n                <span v-else>***</span>\r\n              </div>\r\n              <button-sets @click=\"alterPwdVisibility\">查看密码</button-sets>\r\n            </div>\r\n          </td>\r\n          <td>\r\n            <div>\r\n              <button-sets @click=\"edit(index)\">编辑</button-sets>\r\n              <button-sets @click=\"deleteAdmin(index)\">删除</button-sets>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table-sets>\r\n    <dialog-sets\r\n      v-if=\"formIsVisible\"\r\n      title=\"请确认以下信息：\"\r\n      @close=\"addAdmin\"\r\n    >\r\n      <template #default>\r\n        <form class=\"admin-form\" v-if=\"formIsVisible\" @submit.prevent>\r\n          <label\r\n            >账号：\r\n            <input ref=\"accountInput\" v-model=\"admin.account\" />\r\n          </label>\r\n          <label\r\n            >密码：\r\n            <input ref=\"passwordInput\" v-model=\"admin.password\" />\r\n          </label>\r\n          <label\r\n            >电话：\r\n            <input ref=\"phoneInput\" v-model=\"admin.phone\" />\r\n          </label>\r\n          <label\r\n            >邮箱：\r\n            <input ref=\"emailInput\" v-model=\"admin.email\" />\r\n          </label>\r\n          <label\r\n            >描述：\r\n            <textarea\r\n              rows=\"3\"\r\n              ref=\"descriptionInput\"\r\n              v-model=\"admin.description\"\r\n            />\r\n          </label>\r\n        </form>\r\n      </template>\r\n      <template #actions>\r\n        <button-sets v-if=\"addAdmins\" class=\"pop-up-button\" @click=\"confirm\"\r\n          >确认</button-sets\r\n        >\r\n        <button-sets v-else class=\"pop-up-button\" @click=\"save\">保存</button-sets>\r\n        <button-sets class=\"pop-up-button\" @click=\"close\">关闭</button-sets>\r\n      </template>\r\n    </dialog-sets>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      allChecked: false,\r\n      pwdIsVisible: false,\r\n      formIsVisible: false,\r\n      accountInput: \"\",\r\n      passwordInput: \"\",\r\n      phoneInput: \"\",\r\n      emailInput: \"\",\r\n      descriptionInput: \"\",\r\n      admins: [\r\n        {\r\n          checked: false,\r\n          id: 1,\r\n          account: \"admin1\",\r\n          password: \"123456\",\r\n          role: \"管理员\",\r\n          status: true,\r\n          phone: \"18915464612\",\r\n          email: \"admin1@cats.com\",\r\n      