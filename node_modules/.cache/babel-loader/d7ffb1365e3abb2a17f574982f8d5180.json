{"ast":null,"code":"export default {\n  data() {\n    return {\n      adminAccount: \"admin1\",\n      adminPwd: \"123456\",\n      contentIsVisible: false\n    };\n  },\n\n  methods: {\n    logIn() {\n      if ((this.adminAccount === \"admin1\" || this.adminAccount === \"admin2\" || this.adminAccount === \"admin3\" || this.adminAccount === \"admin4\") && this.adminPwd === \"123456\") {\n        this.$router.push(\"/home\");\n      } else if ((this.adminAccount !== \"admin1\" || this.adminAccount !== \"admin2\" || this.adminAccount !== \"admin3\" || this.adminAccount !== \"admin4\") && this.adminPwd === \"123456\") {\n        alert(\"账号错误，请输入正确的账号\");\n      } else if ((this.adminAccount === \"admin1\" || this.adminAccount === \"admin2\" || this.adminAccount === \"admin3\" || this.adminAccount === \"admin4\") && this.adminPwd !== \"123456\") {\n        alert(\"密码错误，请输入正确的密码\");\n      } else {\n        alert(\"账号和密码错误，请输入正确的账号和密码\");\n      }\n    },\n\n    alterContentVisibility() {\n      this.contentIsVisible = !this.contentIsVisible;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyCA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,QADT;MAELC,QAAQ,EAAE,QAFL;MAGLC,gBAAgB,EAAE;IAHb,CAAP;EAKD,CAPY;;EAQbC,OAAO,EAAE;IACPC,KAAK,GAAG;MACN,IACE,CAAC,KAAKJ,YAAL,KAAsB,QAAtB,IACC,KAAKA,YAAL,KAAsB,QADvB,IAEC,KAAKA,YAAL,KAAsB,QAFvB,IAGC,KAAKA,YAAL,KAAsB,QAHxB,KAIA,KAAKC,QAAL,KAAkB,QALpB,EAME;QACA,KAAKI,OAAL,CAAaC,IAAb,CAAkB,OAAlB;MACF,CARA,MAQO,IACL,CAAC,KAAKN,YAAL,KAAsB,QAAtB,IACC,KAAKA,YAAL,KAAsB,QADvB,IAEC,KAAKA,YAAL,KAAsB,QAFvB,IAGC,KAAKA,YAAL,KAAsB,QAHxB,KAIA,KAAKC,QAAL,KAAkB,QALb,EAML;QACAM,KAAK,CAAC,eAAD,CAAL;MACF,CARO,MAQA,IACL,CAAC,KAAKP,YAAL,KAAsB,QAAtB,IACC,KAAKA,YAAL,KAAsB,QADvB,IAEC,KAAKA,YAAL,KAAsB,QAFvB,IAGC,KAAKA,YAAL,KAAsB,QAHxB,KAIA,KAAKC,QAAL,KAAkB,QALb,EAML;QACAM,KAAK,CAAC,eAAD,CAAL;MACF,CARO,MAQA;QACLA,KAAK,CAAC,qBAAD,CAAL;MACF;IACD,CA7BM;;IA8BPC,sBAAsB,GAAG;MACvB,KAAKN,gBAAL,GAAwB,CAAC,KAAKA,gBAA9B;IACD;;EAhCM;AARI,CAAf","names":["data","adminAccount","adminPwd","contentIsVisible","methods","logIn","$router","push","alert","alterContentVisibility"],"sourceRoot":"","sources":["D:\\vue\\Mao\\src\\components\\layouts\\LogIn.vue"],"sourcesContent":["<template>\r\n  <div class=\"log-in-wrapper\">\r\n    <section>\r\n      <h1>猫咪管理系统</h1>\r\n      <span>欢迎回来!</span>\r\n      <div class=\"login\">\r\n        <h2>管理员登录</h2>\r\n        <div>\r\n          <div>\r\n            <label>\r\n              <svg aria-hidden=\"true\" class=\"user\">\r\n                <use xlink:href=\"#icon-maomao\"></use>\r\n              </svg>\r\n              <input type=\"text\" placeholder=\"账号\" v-model=\"adminAccount\" />\r\n            </label>\r\n          </div>\r\n          <div>\r\n            <label>\r\n              <svg aria-hidden=\"true\" class=\"pwd\">\r\n                <use xlink:href=\"#icon-maoliang1\"></use>\r\n              </svg>\r\n              <input type=\"password\" placeholder=\"密码\" v-model=\"adminPwd\" />\r\n            </label>\r\n          </div>\r\n          <button type=\"button\" @click=\"logIn\">登录</button>\r\n          <button @click=\"alterContentVisibility\">账号提示</button>\r\n          <div v-if=\"contentIsVisible\" class=\"hidden\">\r\n            <span\r\n              >管理员账号：\r\n              <em>admin1</em>\r\n              或 <em>admin2</em> 或 <em>admin3</em> 或 <em>admin4</em></span\r\n            >\r\n            <span>密码都是：<em>123456</em></span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      adminAccount: \"admin1\",\r\n      adminPwd: \"123456\",\r\n      contentIsVisible: false,\r\n    };\r\n  },\r\n  methods: {\r\n    logIn() {\r\n      if (\r\n        (this.adminAccount === \"admin1\" ||\r\n          this.adminAccount === \"admin2\" ||\r\n          this.adminAccount === \"admin3\" ||\r\n          this.adminAccount === \"admin4\") &&\r\n        this.adminPwd === \"123456\"\r\n      ) {\r\n        this.$router.push(\"/home\");\r\n      } else if (\r\n        (this.adminAccount !== \"admin1\" ||\r\n          this.adminAccount !== \"admin2\" ||\r\n          this.adminAccount !== \"admin3\" ||\r\n          this.adminAccount !== \"admin4\") &&\r\n        this.adminPwd === \"123456\"\r\n      ) {\r\n        alert(\"账号错误，请输入正确的账号\");\r\n      } else if (\r\n        (this.adminAccount === \"admin1\" ||\r\n          this.adminAccount === \"admin2\" ||\r\n          this.adminAccount === \"admin3\" ||\r\n          this.adminAccount === \"admin4\") &&\r\n        this.adminPwd !== \"123456\"\r\n      ) {\r\n        alert(\"密码错误，请输入正确的密码\");\r\n      } else {\r\n        alert(\"账号和密码错误，请输入正确的账号和密码\");\r\n      }\r\n    },\r\n    alterContentVisibility() {\r\n      this.contentIsVisible = !this.contentIsVisible;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.log-in-wrapper {\r\n  width: 100%;\r\n  height: 1080px;\r\n  cursor: url(\"../../assets/ico/meme-pop-cat-cursor-close.ico\"), default !important;\r\n  padding-top: 170px;\r\n  background: url(\"../../assets/12.png\") no-repeat 50%;\r\n  background-position-y: 550px;\r\n}\r\nsection {\r\n  width: 500px;\r\n  margin: 0 auto;\r\n  box-shadow: 0 0 20px 0 rgba(255, 0, 157, 0.493);\r\n  text-align: center;\r\n  box-sizing: border-box;\r\n  background: #ad9aaa3a;\r\n  border-radius: 15px;\r\n}\r\nh1 {\r\n  font-size: 28px;\r\n  color: rgba(255, 0, 157, 0.473);\r\n  padding-top: 20px;\r\n}\r\nspan {\r\n  margin-top: -10px;\r\n  display: block;\r\n  margin-bottom: 10px;\r\n  color: rgba(255, 0, 157, 0.4);\r\n  font-size: 16px;\r\n  font-weight: bold;\r\n}\r\n.login {\r\n  border-top: 1px solid rgba(255, 0, 157, 0.37);\r\n}\r\nh2 {\r\n  font-size: 21px;\r\n  color: rgba(255, 0, 157, 0.473);\r\n}\r\ninput {\r\n  border: 1px solid rgba(255, 0, 157, 0.301);\r\n  border-radius: 3px;\r\n  margin-bottom: 8px;\r\n  height: 23px;\r\n  color: rgba(255, 0, 157, 0.527);\r\n  font-weight: bold;\r\n  text-indent: 21px;\r\n  margin-left: -20px;\r\n}\r\ninput:hover {\r\n  cursor: url(\"../../assets/ico/meme-pop-cat-pointer-open.ico\"), pointer !important;\r\n}\r\ninput:focus {\r\n  border: 1px solid white;\r\n  outline: none;\r\n  background: rgb(243, 243, 172);\r\n}\r\ninput::placeholder {\r\n  color: rgba(255, 0, 157, 0.342);\r\n}\r\nsvg:hover {\r\n  cursor: url(\"../../assets/ico/meme-pop-cat-pointer-open.ico\"), pointer !important;\r\n}\r\nbutton {\r\n  display: inline-block;\r\n  text-decoration: none;\r\n  margin: 15px auto;\r\n  margin-bottom: 25px;\r\n  width: 86px;\r\n  height: 40px;\r\n  line-height: 40px;\r\n  border-radius: 10px;\r\n  font-size: 16px;\r\n  color: rgba(255, 0, 157, 0.371);\r\n  background: rgba(243, 243, 172, 0.466);\r\n  font-weight: bold;\r\n  border: 1px solid rgba(255, 0, 157, 0.301);\r\n  margin-right: 10px;\r\n}\r\nbutton:hover {\r\n  cursor: url(\"../../assets/ico/woman_yelling_at_a_cat_meme_pointer-cat.ico\"),\r\n    default;\r\n  outline: none;\r\n  border: 1px solid yellow;\r\n  color: rgba(255, 0, 157, 0.527);\r\n  background: rgba(255, 255, 0, 0.424);\r\n}\r\nh3 {\r\n  line-height: 0;\r\n}\r\n.user {\r\n  position: relative;\r\n  top: 4px;\r\n  width: 15px;\r\n  height: 15px;\r\n}\r\n.pwd {\r\n  position: relative;\r\n  top: 4px;\r\n  width: 15px;\r\n  height: 15px;\r\n}\r\n.hidden span {\r\n  color: #435a64;\r\n  font-weight: lighter;\r\n  text-align: left;\r\n}\r\nem {\r\n  font-weight: bold;\r\n  color: #e56c8f;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}