tton-sets>\r\n            </div>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table-sets>\r\n    <dialog-sets\r\n      v-if=\"formIsVisible\"\r\n      :title=\"addAdmins ? '请确认以下信息：' : '请修改以下信息：'\"\r\n      @close=\"addAdmin\"\r\n    >\r\n      <template #default>\r\n        <form class=\"admin-form\" v-if=\"formIsVisible\" @submit.prevent>\r\n          <label\r\n            >头像：\r\n            <input v-model=\"admin.img\" />\r\n          </label>\r\n          <label\r\n            >账号：\r\n            <input v-model=\"admin.account\" />\r\n          </label>\r\n          <label\r\n            >密码：\r\n            <input v-model=\"admin.password\" />\r\n          </label>\r\n          <label\r\n            >电话：\r\n            <input v-model=\"admin.phone\" />\r\n          </label>\r\n          <label\r\n            >邮箱：\r\n            <input v-model=\"admin.email\" />\r\n          </label>\r\n          <label\r\n            >描述：\r\n            <textarea rows=\"3\" v-model=\"admin.description\" />\r\n          </label>\r\n        </form>\r\n      </template>\r\n      <template #actions>\r\n        <button-sets v-if=\"addAdmins\" class=\"pop-up-button\" @click=\"confirm\"\r\n          >确认</button-sets\r\n        >\r\n        <button-sets v-else class=\"pop-up-button\" @click=\"save\"\r\n          >保存</button-sets\r\n        >\r\n        <button-sets class=\"pop-up-button\" @click=\"close\">关闭</button-sets>\r\n      </template>\r\n    </dialog-sets>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      allChecked: false,\r\n      formIsVisible: false,\r\n      addAdmins: true,\r\n      searchValue: null,\r\n      // accountInput: \"\",\r\n      // passwordInput: \"\",\r\n      // phoneInput: \"\",\r\n      // emailInput: \"\",\r\n      // descriptionInput: \"\",\r\n      idx: null,\r\n      admins: [\r\n        {\r\n          checked: false,\r\n          id: null,\r\n          account: \"admin1\",\r\n     