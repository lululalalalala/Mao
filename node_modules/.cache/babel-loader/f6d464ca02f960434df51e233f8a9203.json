{"ast":null,"code":"export default {\n  data() {\n    return {\n      items: [{\n        id: 1,\n        title: \"铲屎\",\n        list: 1\n      }, {\n        id: 2,\n        title: \"梳毛\",\n        list: 1\n      }, {\n        id: 3,\n        title: \"陪玩\",\n        list: 1\n      }, {\n        id: 4,\n        title: \"量体温\",\n        list: 3\n      }, {\n        id: 5,\n        title: \"添粮\",\n        list: 2\n      }, {\n        id: 6,\n        title: \"换水\",\n        list: 2\n      }]\n    };\n  },\n\n  computed: {\n    listOne() {\n      return this.items.filter(item => item.list === 1);\n    },\n\n    listTwo() {\n      return this.items.filter(item => item.list === 2);\n    },\n\n    listThree() {\n      return this.items.filter(item => item.list === 3);\n    }\n\n  },\n  methods: {\n    dragStart: (evt, item) => {\n      evt.dataTransfer.dropEffect = \"move\";\n      evt.dataTransfer.effectAllowed = \"move\";\n      evt.dataTransfer.setData(\"itemID\", item.id);\n    },\n\n    drop(evt, listID) {\n      const itemID = evt.dataTransfer.getData(\"itemID\");\n      const item = this.items.find(item => item.id == itemID);\n      item.list = listID;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgDA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QACEC,EAAE,EAAE,CADN;QAEEC,KAAK,EAAE,IAFT;QAGEC,IAAI,EAAE;MAHR,CADK,EAML;QACEF,EAAE,EAAE,CADN;QAEEC,KAAK,EAAE,IAFT;QAGEC,IAAI,EAAE;MAHR,CANK,EAWL;QACEF,EAAE,EAAE,CADN;QAEEC,KAAK,EAAE,IAFT;QAGEC,IAAI,EAAE;MAHR,CAXK,EAgBL;QACEF,EAAE,EAAE,CADN;QAEEC,KAAK,EAAE,KAFT;QAGEC,IAAI,EAAE;MAHR,CAhBK,EAqBL;QACEF,EAAE,EAAE,CADN;QAEEC,KAAK,EAAE,IAFT;QAGEC,IAAI,EAAE;MAHR,CArBK,EA0BL;QACEF,EAAE,EAAE,CADN;QAEEC,KAAK,EAAE,IAFT;QAGEC,IAAI,EAAE;MAHR,CA1BK;IADF,CAAP;EAkCD,CApCY;;EAqCbC,QAAQ,EAAE;IACRC,OAAO,GAAG;MACR,OAAO,KAAKL,KAAL,CAAWM,MAAX,CAAmBC,IAAD,IAAUA,IAAI,CAACJ,IAAL,KAAc,CAA1C,CAAP;IACD,CAHO;;IAIRK,OAAO,GAAG;MACR,OAAO,KAAKR,KAAL,CAAWM,MAAX,CAAmBC,IAAD,IAAUA,IAAI,CAACJ,IAAL,KAAc,CAA1C,CAAP;IACD,CANO;;IAORM,SAAS,GAAG;MACV,OAAO